@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<header>
    <h1 class="text-danger">@*@ViewData["Title"]*@Accès refusé</h1>
    <p class="text-danger">@*You do not have access to this resource.*@Vous n'avez pas accès à cette ressource.</p>
</header>
